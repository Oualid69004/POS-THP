<p id="notice"><%= notice %></p>

<h1>Stockins</h1>

<table>
  <thead>
    <tr>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stockins.each do |stockin| %>
      <tr>
        <td><%= stockin.total %></td>
        <td><%= link_to 'Show', stockin %></td>
        <td><%= link_to 'Edit', edit_stockin_path(stockin) %></td>
        <td><%= link_to 'Destroy', stockin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stockin', new_stockin_path %>
