<p id="notice"><%= notice %></p>

<h1>Stocklevels</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Stockmaximum</th>
      <th>Stocksecurity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stocklevels.each do |stocklevel| %>
      <tr>
        <td><%= stocklevel.location %></td>
        <td><%= stocklevel.stockmaximum %></td>
        <td><%= stocklevel.stocksecurity %></td>
        <td><%= link_to 'Show', stocklevel %></td>
        <td><%= link_to 'Edit', edit_stocklevel_path(stocklevel) %></td>
        <td><%= link_to 'Destroy', stocklevel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stocklevel', new_stocklevel_path %>
